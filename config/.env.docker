# Copy this file to .env and fill in your actual values for Docker/docker-compose

# Select one backend: mongo | postgres | supabase
DB_BACKEND=mongo

# Shared table/collection name
STOCK_TABLE=stock_data

# MongoDB settings
MONGODB_URI=mongodb://mongodb:27017/
MONGODB_DATABASE=nse_data

# PostgreSQL settings
SQL_DATABASE_URL=postgresql+psycopg2://postgres:postgres@postgres:5432/nse_data
SQL_ECHO=false

# Supabase settings
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_TABLE=stock_data
